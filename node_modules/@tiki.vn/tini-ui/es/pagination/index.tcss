.content-center,.tu-pagination,.tu-pagination .tu-pagination-jump,.tu-pagination .tu-pagination-number,.tu-pagination .tu-pagination-page{display:flex;flex-direction:row;align-items:center;justify-content:center}.tu-pagination .tu-pagination-jump,.tu-pagination-button{width:40px;height:40px;color:var(--text-paging-inactive);background:0 0;border-radius:8px;font-size:var(--font-size-base);font-style:var(--font-style-normal);cursor:pointer;text-overflow:unset;border:solid 1px transparent}.tu-pagination-button:hover{background:0 0}.tu-pagination .tu-pagination-page{background-color:var(--background-paging-inactive);border-radius:8px;margin:0 8px}.tu-pagination .tu-pagination-page .tu-pagination-button__active{background-color:var(--background-paging-active);color:var(--text-paging-active)}.tu-pagination .tu-pagination-number{min-width:200px;margin:0 8px;font-size:var(--font-size-base);font-weight:var(--font-weight-regular);font-style:var(--font-style-normal)}.tu-pagination .tu-pagination-number .tu-pagination-button__active{font-weight:var(--font-weight-bold);color:var(--background-paging-active)}.tu-pagination .tu-pagination-jump{border:1px solid var(--blue60);background-color:var(--white);border-radius:4px;min-width:40px;box-sizing:border-box}.tu-pagination .tu-pagination-jump__disabled{cursor:not-allowed;border:1px solid var(--gray20)}