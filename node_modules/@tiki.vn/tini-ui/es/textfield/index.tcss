.tu-textfield .t-input-content{caret-color:var(--brand,#1a94ff)}.tu-textfield .tu-textfield-inline{display:inline}.tu-textfield.rounded .tu-textfield-loading{border-radius:var(--border-radius-rounded-4px)}.tu-textfield.pill .tu-textfield-input,.tu-textfield.pill .tu-textfield-loading{border-radius:var(--border-radius-pill)}.tu-textfield.error .tu-textfield-input{border-color:var(--error)}.tu-textfield .tu-textfield-error-message{color:var(--error);font-size:var(--font-size-small);margin-top:var(--space-stack-4x-small)}.tu-textfield .tu-textfield-error-icon{position:absolute;top:calc(var(--space-inset-2x-small) + 2px);padding:0 2px}.tu-textfield .tu-textfield-icon{position:absolute;top:var(--space-inset-2x-small)}.tu-textfield .tu-textfield-icon.right{right:var(--space-inline-2x-small)}.tu-textfield .tu-textfield-icon.left{left:var(--space-inline-2x-small)}.tu-textfield .tu-textfield-inline{position:relative;display:flex;align-items:center}.tu-textfield .tu-textfield-inline .tu-textfield-input{flex:1;background:0 0}.tu-textfield .tu-textfield-loading{position:absolute;left:1px;top:1px;bottom:1px;right:1px;z-index:1}.tu-textfield .tu-textfield-loading-inline{position:relative;flex:1;height:100%;display:flex;align-items:center;padding:0 var(--space-inline-medium)}.tu-textfield .tu-textfield-loading-inline-input{background:var(--background-skeleton);border-radius:var(--border-radius-pill);width:128px;max-width:100%;height:12px;color:transparent}.tu-textfield .tu-textfield-loading-inline-icon{width:24px;height:24px;border-radius:50%;margin-right:var(--space-inline-2x-small);background:var(--background-skeleton)}.tu-textfield .tu-textfield-loading-inline-icon.left{margin-left:calc(var(--space-inline-2x-small) * -1)}.tu-textfield .tu-textfield-loading-inline-icon.right{margin-left:auto;margin-right:calc(var(--space-inline-2x-small) * -1)}